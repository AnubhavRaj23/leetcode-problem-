class Solution {
public:
    ListNode* middleNode(ListNode* head) {
        int  n=0;
        struct ListNode* temp=head;
        while(temp!=NULL)
        {
            n++;
            temp = temp->next;
        }
        int k=n/2;
        struct ListNode*t=head;
        for(int i=0;i<k;i++)
        {
            t=t->next;
        }
        return t;
    }
};
